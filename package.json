{
	"name": "fastify-xsurf",
	"description": "A Fastify plugin that adds CSRF protection to HTTP routes",
	"version": "1.0.10",
	"author": "Daniel Zhang",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/DanielHZhang/fastify-xsurf"
	},
	"contributors": [
		"Daniel Zhang (https://github.com/danielhzhang)"
	],
	"publishConfig": {
		"access": "public"
	},
	"prettier": "@supersede/prettier-config",
	"eslintConfig": {
		"extends": "@supersede"
	},
	"main": "lib/index.js",
