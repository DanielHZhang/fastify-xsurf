		"@types/node": "^16.6.1",
		"eslint": "^7.32.0",
		"fastify": "^4.10.2",
		"fastify-cookie": "^5.3.1",
		"jest": "^27.0.6",
		"prettier": "^2.3.2",
		"ts-jest": "^27.0.4",
		"tsc-suppress": "^1.0.6",
		"typescript": "^4.3.5"
